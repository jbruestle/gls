/*
 * Add two ul288's modulo another
 */
inline void ul288_modadd(ul288 dst, ul288 src1, ul288 src2, mod288 n) {
    ul288_add(dst, src1, src2);
    if (ul288_cmp(dst, n->n) >= 0)
        ul288_sub(dst, dst, n->n);
}

/*
 * Subtract one ul288 from another modulo a third
 */
inline void ul288_modsub(ul288 dst, ul288 src1, ul288 src2, mod288 n) {
    ul288 tr1, tr2;
    ul288_sub(tr1, src1, src2);
    ul288_add(tr2, tr1, n->n);
    if (ul288_cmp(src1, src2) >= 0)
        ul288_set(dst, tr1);
    else
        ul288_set(dst, tr2);
}

